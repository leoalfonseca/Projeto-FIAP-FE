{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "rangeStrategy": "bump",
  "packageRules": [
    {
      "depTypeList": ["devDependencies"],
      "enabled": true,
      "automerge": true,
      "automergeType": "branch"
    },
    {
      "depTypeList": ["dependencies"],
      "enabled": true,
      "automerge": false
    },
    {
      "matchPackageNames": ["node"],
      "allowedVersions": "/^16\\.|^18\\.|^20\\.|^22\\./"
    }
  ]
}
